        "cookie-parser": "^1.4.4",
        "core-js": "^3.4.1",
        "dotenv": "^8.2.0",
        "dotenv-webpack": "^1.7.0",
        "express": "^4.17.1",
        "isomorphic-fetch": "^2.2.1",
        "js-cookie": "^2.2.1",
        "lodash": "^4.17.15",
        "next": "^9.1.4",
        "next-compose-plugins": "^2.2.0",
        "next-images": "^1.2.0",
        "next-redux-wrapper": "^4.0.1",
        "next-seo": "^3.0.0",
        "postcss-easy-import": "^3.0.0",
        "postcss-global-import": "^1.0.6",
        "postcss-loader": "^3.0.0",
