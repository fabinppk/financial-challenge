{
    "name": "project-1",
    "version": "1.0.0",
    "description": "",
    "main": "indexs.js",
    "scripts": {
        "dev": "node server.js",
        "build": "next build ./src;",
        "start": "NODE_ENV=production node server.js",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "eslint": "eslint ./src",
        "stylelint": "stylelint ./src/**/*.scss",
        "lint": "npm run eslint && npm run stylelint",
        "test": "jest --verbose",
        "test:watch": "jest  --verbose --watch",
        "test:coverageWatch": "jest  --coverage --verbose --watch",
        "test:coverage": "jest --coverage --colors -w 1",
        "commit": "npm run test:coverage && npm run build && npm run build-storybook && npm run lint"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run commit",
            "pre-push": "npm run commit"
        }
    },
    "jest": {
        "bail": true,
        "setupFiles": [
            "./enzyme.config.js"
        ],
        "moduleNameMapper": {
            "\\.(svg|jpg|png|jpeg)$": "<rootDir>/empty-module.js",
            "^.+\\.(css|scss|less)$": "identity-obj-proxy",
            "_atoms/(.*)$": "<rootDir>/src/components/atoms/$1",
            "_molecules/(.*)$": "<rootDir>/src/components/molecules/$1",
            "_organisms/(.*)$": "<rootDir>/src/components/organisms/$1",
            "_templates/(.*)$": "<rootDir>/src/components/templates/$1",
            "_config/(.*)$": "<rootDir>/src/config/$1",
            "_utils/(.*)$": "<rootDir>/src/utils/$1",
            "_images/(.*)$": "<rootDir>/src/images/$1",
            "_redux/(.*)$": "<rootDir>/src/redux/$1"
        }
    },
    "keywords": [],
    "author": "nubank",
    "license": "ISC",
    "dependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.6",
        "@babel/preset-env": "^7.9.6",
        "@babel/preset-react": "^7.9.4",
        "@babel/runtime-corejs2": "^7.9.6",
        "@zeit/next-css": "^1.0.1",
        "babel-eslint": "^10.1.0",
        "classnames": "^2.2.6",
        "cookie-parser": "^1.4.5",
        "core-js": "^3.6.5",
        "express": "^4.17.1",
        "isomorphic-fetch": "^2.2.1",
        "js-cookie": "^2.2.1",
        "lodash": "^4.17.15",
        "next": "^9.3.6",
        "next-compose-plugins": "^2.2.0",
        "next-images": "^1.4.0",
        "next-redux-wrapper": "^5.0.0",
        "next-seo": "^4.4.0",
        "node-sass": "^4.14.0",
        "prop-types": "^15.7.2",
        "rc-slider": "^9.2.4",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-redux": "^7.2.0",
        "redux": "^4.0.5",
        "redux-thunk": "^2.3.0",
        "sass": "^1.26.5",
        "sass-resources-loader": "^2.0.3",
        "terser": "^4.6.13",
        "webpack-filter-warnings-plugin": "^1.2.1"
    },
    "devDependencies": {
        "@storybook/addon-links": "^5.3.18",
        "@storybook/addon-viewport": "^5.3.18",
        "@storybook/addons": "^5.3.18",
        "@storybook/react": "^5.3.18",
        "babel-jest": "^25.5.1",
        "babel-loader": "^8.1.0",
        "babel-preset-react": "^6.24.1",
        "cross-env": "^7.0.2",
        "css-loader": "^3.5.3",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-react": "^7.19.0",
        "husky": "^4.2.5",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^25.5.2",
        "prettier": "^2.0.5",
        "style-loader": "^1.2.1",
        "stylelint": "^13.3.3",
        "stylelint-config-recommended-scss": "^4.2.0",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-scss": "^3.17.1"
    }
